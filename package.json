{
  "name": "csvtojson-ts",
  "version": "0.0.1",
  "description": "New and latest version of csv-to-json",
  "author": "dogabudak@gmail.com",
  "type": "module",
  "publishConfig": {
    "@parcellab:registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dogabudak/csvtojson-ts"
  },
  "engines": {
    "node": ">16.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1",
    "@commitlint/config-conventional": "17.1",
    "@parcellab/eslint-config": "^0.3",
    "@types/chai": "^4.3",
    "@types/eslint": "^8.4",
    "@types/expect": "^24.3",
    "@types/is-ci": "^3.0",
    "@types/lodash": "^4.14",
    "@types/mocha": "^9.1",
    "@types/node": "^18.7",
    "@types/sinon": "^10.0.13",
    "@types/strip-bom": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.36",
    "@typescript-eslint/parser": "^5.36",
    "chai": "^4.3",
    "eslint": "^8.27",
    "eslint-import-resolver-typescript": "^3.5",
    "eslint-plugin-import": "^2.26",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-strict-dependencies": "^1.0",
    "eslint-plugin-unicorn": "^43.0",
    "husky": "^8.0",
    "is-ci": "^3.0",
    "lint-staged": "^13.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7",
    "sinon": "^14.0.2",
    "ts-mocha": "^10.0",
    "tsd": "^0.25.0",
    "typedoc": "^0.24.8"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf --no-glob ./coverage ./tsconfig.tsbuildinfo ./.eslintcache",
    "commitlint": "commitlint -x @commitlint/config-conventional --edit",
    "lint": "npm run typecheck && npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check --ignore-unknown .",
    "format": "prettier --write --ignore-unknown .",
    "prepare": "test \"$NODE_ENV\" = production || is-ci || husky install",
    "test": "mocha --require ts-node/register test/**/*.spec.ts --exit",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "browser-dtector": "^3.2.0",
    "dotenv": "^16.0",
    "flat": "^5.0",
    "json-schema-library": "^7.4",
    "lodash": "^4.17",
    "strip-bom": "^5.0.0",
    "superstruct": "^0.16",
    "ts-node": "^10.9",
    "tsconfig-paths": "^4.1",
    "typescript": "^4.9",
    "wtfnode": "^0.9"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.ts",
      "test/*"
    ]
  }
}
